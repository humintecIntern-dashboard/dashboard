<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.humintecTest.dashboard.dao.ScannerSlideListDao">
    <select id = "insertScanner" parameterType="ScannerSlideVo" resultType="ScannerSlideVo" >
        select scanner_type, count(*) as slide_sum
        from slide_list
        group by scanner_type
    </select>
</mapper>